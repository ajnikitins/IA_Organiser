<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="400.0" prefWidth="600.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER">
      <children>
        <Label alignment="CENTER" text="Today is: " textAlignment="CENTER">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
         <Label fx:id="dateLabel" alignment="CENTER" textAlignment="CENTER">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
      </children>
   </HBox>
   <Accordion fx:id="taskAccordion">
     <panes>
       <TitledPane animated="false" text="untitled 1" />
       <TitledPane animated="false" text="untitled 2" />
     </panes>
   </Accordion>
</fx:root>
